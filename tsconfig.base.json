{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "baseUrl": ".",
    "paths": {
      "@~~_starter.org_name_~~/data": ["libs/data/src/index.ts"],
      "@~~_starter.org_name_~~/feature-flags": [
        "libs/features/feature-flags/src/index.ts"
      ],
      "@~~_starter.org_name_~~/manage-list-feature": [
        "libs/manage-list/feature/src/index.ts"
      ],
      "@~~_starter.org_name_~~/ui": ["libs/ui/src/index.ts"],
      "@~~_starter.org_name_~~/util": ["libs/util/src/index.ts"]
    }
  }
}
